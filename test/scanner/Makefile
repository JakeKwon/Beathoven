# Beathoven: tests for scanner makefile
# builds tokenize executeable for printing scanned tokens from stdin

OCAMLC = ocamlc
OBJS = ../../src/_build/scanner.cmo tokenize.cmo
INCLUDES = -I ../../src/_build # -I ../../src

default: tokenize

all:
	cd ..; make all

tokenize: $(OBJS)
	$(OCAMLC) $(INCLUDES) -o tokenize $(OBJS)

%.cmo: %.ml
	$(OCAMLC) $(INCLUDES) -c $<

%.cmi: %.mli
	$(OCAMLC) $(INCLUDES) -c $<

.PHONY: clean
clean:
	rm -f tokenize *.cmo *.cmi

# Generated by ocamldep *.ml
tokenize.cmo:
tokenize.cmx:
